<!DOCTYPE html>
<html>
  <head>
    <title>TimeBank</title>
    <%= javascript_include_tag  'touch_ui' %>
    <%= javascript_include_tag  'jquery.min', 'jquery-ui.min','jquery_ujs' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
    <%= yield(:head) %>
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/header' %>
      <section class="round">
        <%= yield %>
      </section>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
