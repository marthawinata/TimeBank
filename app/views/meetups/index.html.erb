<h1>Listing meetups</h1>

<table>
  <tr>
    <th>Host player</th>
    <th>Confirmed date</th>
    <th>Confirmed time</th>
    <th>Confirmed venue</th>
    <th>Meetup type</th>
    <th>Venue link</th>
    <th>Invitation type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meetups.each do |meetup| %>
  <tr>
    <td><%= meetup.host_player_id %></td>
    <td><%= meetup.confirmed_date %></td>
    <td><%= meetup.confirmed_time %></td>
    <td><%= meetup.confirmed_venue %></td>
    <td><%= meetup.meetup_type %></td>
    <td><%= meetup.venue_link %></td>
    <td><%= meetup.invitation_type %></td>
    <td><%= link_to 'Show', meetup %></td>
    <td><%= link_to 'Edit', edit_meetup_path(meetup) %></td>
    <td><%= link_to 'Destroy', meetup, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meetup', new_meetup_path %>
