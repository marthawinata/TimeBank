<p id="notice"><%= notice %></p>

<p>
  <b>Name</b>
  <%= @meetup.name %>
</p>

<p>
  <b>Host User</b>
  <%= @meetup.host_user.email unless @meetup.host_user.nil? %>
</p>

<p>
  <b>Confirmed date:</b>
  <%= @meetup.confirmed_date %>
</p>

<p>
  <b>Confirmed time:</b>
  <%= @meetup.confirmed_time %>
</p>

<p>
  <b>Confirmed venue:</b>
  <%= @meetup.confirmed_venue %>
</p>

<p>
  <b>Meetup type:</b>
  <%= @meetup.meetup_type %>
</p>

<p>
  <b>Venue link:</b>
  <%= @meetup.venue_link %>
</p>

<p>
  <b>Invitation type:</b>
  <%= @meetup.invitation_type %>
</p>


<%= link_to 'Edit', edit_meetup_path(@meetup) %> |
<%= link_to 'Back', meetups_path %>
